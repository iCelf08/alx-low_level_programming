#include "main.h"
/**
* print_minutes_of_day - Function that prints every minute of the day
* from 00:00 to 23:59
*/
void print_minutes_of_day()
{
int hour, minute;
for (hour = 0; hour < 24; hour++)
{
for (minute = 0; minute < 60; minute++)
{
_putchar((hour / 10) + '0');
_putchar((hour % 10) + '0');
_putchar(':');
_putchar((minute / 10) + '0');
_putchar((minute % 10) + '0');
_putchar('\n');
}
}
}
